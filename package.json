{
    "name": "prolog2curry",
    "version": "2.0.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "A tool to transform Prolog programs to Curry programs",
    "category": [ "Metaprogramming" ],
    "dependencies": {
        "base"          :   ">= 3.2.0, < 4.0.0",
        "abstract-curry":   ">= 4.0.0, < 5.0.0",
        "directory"     :   ">= 3.0.0, < 4.0.0",
        "filepath"      :   ">= 3.0.0, < 4.0.0",
        "process"       :   ">= 3.0.0, < 4.0.0",
        "prolog"        :   ">= 3.0.0, < 4.0.0"
    },
    "exportedModules": [ "Language.Prolog.Read" ],
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "executable": {
        "name"   : "pl2curry",
        "main"   : "Main"
    },
    "source": {
        "git": "https://github.com/curry-packages/prolog2curry.git",
        "tag": "$version"
    }
}
